from pyspark import  SparkContext 

from pyspark import SparkConf 

#spark conf 

conf = ( SparkConf() 

        .setMaster("local[*]") 

        .setAppName('pyspark') 

        ) 

sc = SparkContext(conf=conf)


from pyspark.sql import SQLContext

sc = SparkContex.getOrCreate()

sqlContext = SQLContext(sc)
